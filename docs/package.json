{
    "name": "@rachelallyson/hero-hook-form-docs",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "next dev",
        "dev:with-search": "npm run index:search && next dev",
        "index:search": "npx pagefind --site .next/server/app --output-path public/_pagefind 2>/dev/null || echo 'Search index will be generated after first build'",
        "build": "next build",
        "postbuild": "if [ -d 'out' ]; then npx pagefind --site out --output-path out/_pagefind; elif [ -d '.next/server/app' ]; then npx pagefind --site .next/server/app --output-path public/_pagefind; else echo 'No build output found for search indexing'; fi",
        "start": "next start"
    },
    "dependencies": {
        "@heroui/autocomplete": "^2",
        "@heroui/button": "^2.2.27",
        "@heroui/card": "^2",
        "@heroui/checkbox": "^2.3.27",
        "@heroui/date-input": "^2.3.27",
        "@heroui/date-picker": "^2.3.28",
        "@heroui/input": "^2.4.28",
        "@heroui/link": "^2",
        "@heroui/navbar": "^2",
        "@heroui/radio": "^2.3.27",
        "@heroui/react": "^2",
        "@heroui/select": "^2.4.28",
        "@heroui/slider": "^2.4.24",
        "@heroui/spinner": "^2.2.24",
        "@heroui/switch": "^2.2.24",
        "@heroui/system": "^2.4.23",
        "@heroui/theme": "^2",
        "@hookform/resolvers": "^5.2.2",
        "@rachelallyson/hero-hook-form": "^2.0.0",
        "@rachelallyson/heroui-font-picker": "^1.0.0",
        "clsx": "^2.1.1",
        "next": "^16.0.1",
        "next-themes": "^0.4.4",
        "nextra": "^4.6.0",
        "nextra-theme-docs": "^4.6.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hook-form": "^7.65.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4",
        "@types/node": "^24.9.2",
        "@types/react": "^19.2.0",
        "@types/react-dom": "^19.2.0",
        "pagefind": "^1.4.0",
        "tailwindcss": "^4",
        "typescript": "^5.9.3"
    }
}